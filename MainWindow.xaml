<Window x:Class="Events2._0.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Events2._0"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Icon="/icon.ico" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="60"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <Grid Grid.Row="0" Background="{StaticResource backgroundSecond}">
            <Button x:Name="btnBack" HorizontalAlignment="Right" Content="Выход" Click="btnBack_Click"></Button>
            <Image Source="/Resources/logo.png" Width="45" Height="45" HorizontalAlignment="Left" Margin="5"></Image>
            
        </Grid>

        <Frame x:Name="FrmMain" Grid.Row="1" NavigationUIVisibility="Hidden" ContentRendered="FrmMain_ContentRendered"/>
    </Grid>
</Window>
